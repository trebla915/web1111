{
  "cli": {
    "version": ">=7.0.0",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium",
        "buildConfiguration": "Debug",
        "image": "macos-sequoia-15.1-xcode-16.1"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "resourceClass": "medium"
      },
      "env": {
        "EAS_PROJECT_ID": "e3775235-7f75-42c8-906e-8171c4a1e54b"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "environment": "preview",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "image": "macos-sequoia-15.1-xcode-16.1"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "resourceClass": "medium"
      },
      "env": {
        "EAS_PROJECT_ID": "e3775235-7f75-42c8-906e-8171c4a1e54b"
      },
      "autoIncrement": true
    },
    "preview-device": {
      "distribution": "internal",
      "channel": "preview",
      "environment": "preview",
