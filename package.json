{
  "name": "club1111",
  "main": "expo-router/entry",
  "version": "1.0.8",
  "types": "./src/types/env.d.ts",
  "private": true,
  "overrides": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "resolutions": {
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.17",
    "postcss-nested": "^6.0.1"
  },
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "version:patch": "npm version patch && git push && git push --tags",
    "version:minor": "npm version minor && git push && git push --tags",
    "version:major": "npm version major && git push && git push --tags",
    "setup:env": "node ./scripts/setup-env.js",
    "build:ios": "npm run setup:env && eas build --platform ios --profile production",
    "build:android": "npm run setup:env && eas build --platform android --profile production",
    "submit:ios": "npm run setup:env && eas submit --platform ios --profile production",
    "submit:android": "npm run setup:env && eas submit --platform android --profile production",
    "build:all": "npm run build:ios && npm run build:android",
    "submit:all": "npm run submit:ios && npm run submit:android",
    "dev:ios": "eas build --profile development --platform ios && eas build:run --platform ios",
    "dev:android": "eas build --profile development --platform android && eas build:run --platform android"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
