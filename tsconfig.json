{
  "extends": "./node_modules/@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "typeRoots": [
      "./node_modules/@types",
      "./src/types"
    ],
    "paths": {
      "@/*": ["src/*"]
    },
    "noEmit": true
  },
  "include": [
    "app/**/*",
    "src/**/*",
    "src/types/env.d.ts"
  ]
}